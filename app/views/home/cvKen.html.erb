        el.style.background = `linear-gradient(135deg, ${backgroundColor} 0%, ${secondaryColor} 100%)`;
      });

      const cards = document.querySelectorAll('.card');
      cards.forEach(card => {
        card.style.background = surfaceColor;
      });

      const textElements = document.querySelectorAll('h1, h2, h3, p:not(.text-white):not(.text-blue-100):not(.text-blue-600)');
      textElements.forEach(el => {
        if (!el.classList.contains('text-white') && !el.classList.contains('text-blue-100') && !el.classList.contains('text-blue-600')) {
          el.style.color = textColor;
        }
      });

      const skillFills = document.querySelectorAll('.skill-fill');
      skillFills.forEach(fill => {
        fill.style.background = `linear-gradient(90deg, ${primaryColor}, ${secondaryColor})`;
