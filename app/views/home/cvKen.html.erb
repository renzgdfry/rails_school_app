      position: relative;
      box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
    }
    
    .contact-item:hover {
      transform: translateY(-8px) scale(1.03);
      box-shadow: 0 20px 50px rgba(102, 126, 234, 0.25);
      background: linear-gradient(135deg, rgba(102, 126, 234, 0.05), rgba(118, 75, 162, 0.05));
    }
    
    .contact-icon-box {
      width: 70px;
      height: 70px;
      background: linear-gradient(135deg, #667eea, #764ba2);
      border-radius: 15px;
      display: flex;
      align-items: center;
      justify-content: center;
      flex-shrink: 0;
      font-size: 2rem;
      transition: all 0.3s ease;
      position: relative;
      pointer-events: none;
    }
    
    .contact-icon-box::after {
      content: '';
      position: absolute;
      inset: -3px;
      background: linear-gradient(135deg, #667eea, #764ba2);
      border-radius: 15px;
      opacity: 0;
      filter: blur(10px);
      transition: opacity 0.3s ease;
      z-index: -1;
      pointer-events: none;
    }
    
    .contact-item:hover .contact-icon-box {
      transform: rotate(10deg) scale(1.1);
    }
    
    .contact-item:hover .contact-icon-box::after {
      opacity: 0.7;
    }
    
    .contact-info {
      flex: 1;
      pointer-events: none;
    }
    
    .contact-info h4 {
      font-size: 1.1rem;
      font-weight: 700;
      color: #667eea;
      margin-bottom: 0.3rem;
      pointer-events: none;
    }
    
    .contact-info p {
      margin: 0;
      color: #666;
      font-size: 0.95rem;
      pointer-events: none;
    }
    
    .contact-info .contact-value {
      font-weight: 600;
      color: #333;
      font-size: 1rem;
      pointer-events: none;
    }
    
    /* CTA Section */
    .cta-section {
      background: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
      border-radius: 25px;
      padding: 4rem;
      text-align: center;
      color: white;
      box-shadow: 0 20px 60px rgba(245, 87, 108, 0.4);
      position: relative;
      overflow: hidden;
    }
    
    .cta-section::before {
      content: '';
      position: absolute;
      top: -50%;
      right: -50%;
      width: 200%;
      height: 200%;
      background: radial-gradient(circle, rgba(255,255,255,0.1) 0%, transparent 70%);
      animation: rotate 20s linear infinite;
    }
    
    @keyframes rotate {
      from { transform: rotate(0deg); }
      to { transform: rotate(360deg); }
    }
    
    .cta-content {
      position: relative;
      z-index: 1;
    }
    
    .btn-primary-custom {
      background: white;
      color: #f5576c;
      border: none;
      padding: 1rem 3rem;
      border-radius: 50px;
      font-weight: 700;
      font-size: 1.1rem;
      transition: all 0.3s ease;
      box-shadow: 0 10px 30px rgba(0,0,0,0.2);
    }
    
    .btn-primary-custom:hover {
      transform: scale(1.1);
      box-shadow: 0 15px 40px rgba(0,0,0,0.3);
      background: #fff;
      color: #f5576c;
    }
    
    .section-title {
      font-size: 2.5rem;
      font-weight: 800;
      text-align: center;
      margin-bottom: 1rem;
      position: relative;
      display: inline-block;
    }
    
    .section-subtitle {
      text-align: center;
      font-size: 1.1rem;
      color: #666;
      margin-bottom: 3rem;
    }
  </style>
