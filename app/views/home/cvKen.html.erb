      position: absolute;
      top: -50%;
      left: -50%;
      width: 200%;
      height: 200%;
      background: radial-gradient(circle at top left, rgba(255,255,255,0.1) 0%, transparent 50%);
      animation: gradientShift 15s ease infinite;
    }
    
    @keyframes gradientShift {
      0%, 100% { transform: translate(0, 0); }
      50% { transform: translate(10%, 10%); }
    }
    
    .contact-left h2 {
      font-size: 2.5rem;
      font-weight: 800;
      margin-bottom: 1rem;
      position: relative;
      z-index: 1;
    }
    
    .contact-left p {
      font-size: 1.1rem;
      opacity: 0.95;
      margin-bottom: 2rem;
      position: relative;
      z-index: 1;
    }
    
    .contact-right {
      background: white;
      padding: 3rem;
    }
    
    .contact-item {
