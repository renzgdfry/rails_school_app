      box-sizing: border-box;
      margin: 0;
      padding: 0;
    }
    
    #app-wrapper {
      width: 100%;
      height: 100%;
      overflow: auto;
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    }
    
    /* Hero Section with Gradient */
    .hero-section {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      padding: 6rem 0;
      text-align: center;
      color: white;
      position: relative;
      overflow: hidden;
    }
    
    .hero-section::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="rgba(255,255,255,0.1)" d="M0,96L48,112C96,128,192,160,288,160C384,160,480,128,576,122.7C672,117,768,139,864,138.7C960,139,1056,117,1152,101.3C1248,85,1344,75,1392,69.3L1440,64L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>');
      background-size: cover;
      opacity: 0.3;
    }
    
    .hero-content {
      position: relative;
      z-index: 1;
    }
    
    .hero-title {
      font-size: 3.5rem;
      font-weight: 800;
      margin-bottom: 1rem;
      text-shadow: 2px 2px 4px rgba(0,0,0,0.2);
      animation: fadeInUp 1s ease-out;
    }
    
    .hero-subtitle {
      font-size: 1.5rem;
      font-weight: 300;
      margin-bottom: 2rem;
      animation: fadeInUp 1.2s ease-out;
    }
    
    .trait-badge {
      display: inline-block;
      padding: 0.75rem 1.5rem;
      background: rgba(255, 255, 255, 0.2);
      backdrop-filter: blur(10px);
      color: white;
      border: 2px solid rgba(255, 255, 255, 0.3);
      border-radius: 30px;
      font-size: 0.95rem;
      font-weight: 600;
      margin: 0.5rem;
      transition: all 0.3s ease;
      animation: float 3s ease-in-out infinite;
    }
    
    .trait-badge:nth-child(1) { animation-delay: 0s; }
    .trait-badge:nth-child(2) { animation-delay: 0.5s; }
    .trait-badge:nth-child(3) { animation-delay: 1s; }
    .trait-badge:nth-child(4) { animation-delay: 1.5s; }
    
    .trait-badge:hover {
      background: rgba(255, 255, 255, 0.3);
      transform: translateY(-5px) scale(1.05);
      box-shadow: 0 10px 25px rgba(0,0,0,0.2);
    }
    
    @keyframes float {
      0%, 100% { transform: translateY(0px); }
      50% { transform: translateY(-10px); }
    }
    
    @keyframes fadeInUp {
      from {
        opacity: 0;
        transform: translateY(30px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
    }
    
    .intro-card {
      background: white;
      border-radius: 20px;
      padding: 3rem;
      margin-top: -4rem;
      box-shadow: 0 20px 60px rgba(0,0,0,0.3);
      position: relative;
      z-index: 2;
    }
    
    /* Zigzag Layout for Strengths */
    .strength-section {
      background: white;
      padding: 5rem 0;
    }
    
    .strength-item {
      display: flex;
      align-items: center;
      margin-bottom: 4rem;
      gap: 3rem;
    }
    
    .strength-item:nth-child(even) {
      flex-direction: row-reverse;
    }
    
    .strength-icon {
      flex-shrink: 0;
      width: 120px;
      height: 120px;
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      border-radius: 30px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 3.5rem;
      box-shadow: 0 10px 30px rgba(102, 126, 234, 0.4);
      transition: all 0.3s ease;
      animation: pulse 2s infinite;
    }
    
    .strength-item:hover .strength-icon {
      transform: rotate(10deg) scale(1.1);
    }
    
    @keyframes pulse {
      0%, 100% { box-shadow: 0 10px 30px rgba(102, 126, 234, 0.4); }
      50% { box-shadow: 0 10px 40px rgba(102, 126, 234, 0.6); }
    }
    
    .strength-content {
      flex: 1;
      background: #f8f9fa;
      padding: 2rem;
      border-radius: 15px;
      border-left: 4px solid #667eea;
    }
    
    .strength-item:nth-child(even) .strength-content {
      border-left: none;
      border-right: 4px solid #764ba2;
    }
    
    /* Experience Section with Card Layout */
    .experience-section {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      padding: 5rem 0;
      position: relative;
      overflow: hidden;
    }
    
    .experience-section::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="rgba(255,255,255,0.05)" d="M0,96L48,128C96,160,192,224,288,240C384,256,480,224,576,197.3C672,171,768,149,864,154.7C960,160,1056,192,1152,197.3C1248,203,1344,181,1392,170.7L1440,160L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>');
      background-size: cover;
      opacity: 0.3;
    }
    
    .experience-section .section-title {
      color: white;
    }
    
    .experience-section .section-subtitle {
      color: rgba(255, 255, 255, 0.9);
    }
    
    .experience-card {
      background: white;
      border-radius: 25px;
      padding: 2.5rem;
      height: 100%;
      position: relative;
      overflow: hidden;
      transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
      box-shadow: 0 10px 40px rgba(0, 0, 0, 0.2);
      cursor: pointer;
    }
    
    .experience-card::before {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 5px;
      background: linear-gradient(90deg, #667eea, #764ba2);
      transform: scaleX(0);
      transform-origin: left;
      transition: transform 0.5s ease;
    }
    
    .experience-card:hover::before {
      transform: scaleX(1);
    }
    
    .experience-card:hover {
      transform: translateY(-15px) scale(1.03);
      box-shadow: 0 25px 60px rgba(102, 126, 234, 0.4);
    }
    
    .experience-icon-wrapper {
      width: 80px;
      height: 80px;
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      border-radius: 20px;
      display: flex;
      align-items: center;
      justify-content: center;
      margin-bottom: 1.5rem;
      position: relative;
      transition: all 0.4s ease;
    }
    
    .experience-card:hover .experience-icon-wrapper {
      transform: rotate(10deg) scale(1.1);
      box-shadow: 0 10px 30px rgba(102, 126, 234, 0.5);
    }
    
    .experience-icon {
      font-size: 2.5rem;
      animation: floatIcon 3s ease-in-out infinite;
    }
    
    @keyframes floatIcon {
      0%, 100% { transform: translateY(0px); }
      50% { transform: translateY(-8px); }
    }
    
    .experience-content {
      position: relative;
      z-index: 1;
    }
    
    .experience-title {
      font-size: 1.4rem;
      font-weight: 700;
      color: #667eea;
      margin-bottom: 1rem;
      transition: color 0.3s ease;
    }
    
    .experience-card:hover .experience-title {
      color: #764ba2;
    }
    
    .experience-description {
      color: #555;
      line-height: 1.7;
      margin-bottom: 1.5rem;
      font-size: 0.95rem;
    }
    
    .experience-tags {
      display: flex;
      flex-wrap: wrap;
      gap: 0.5rem;
    }
    
    .exp-tag {
      display: inline-block;
      padding: 0.4rem 1rem;
      background: linear-gradient(135deg, #f5f7fa, #c3cfe2);
      color: #667eea;
      border-radius: 20px;
      font-size: 0.75rem;
      font-weight: 600;
      transition: all 0.3s ease;
    }
    
    .experience-card:hover .exp-tag {
      background: linear-gradient(135deg, #667eea, #764ba2);
      color: white;
      transform: translateY(-2px);
    }
    
    .experience-number {
      position: absolute;
      bottom: 1rem;
      right: 1.5rem;
      font-size: 4rem;
      font-weight: 900;
      color: rgba(102, 126, 234, 0.08);
      transition: all 0.3s ease;
      z-index: 0;
    }
    
    .experience-card:hover .experience-number {
      color: rgba(102, 126, 234, 0.15);
      transform: scale(1.2);
    }
    
    /* Hexagonal Skills Layout */
    .skills-section {
      background: linear-gradient(135deg, #f5f7fa 0%, #c3cfe2 100%);
      padding: 5rem 0;
    }
    
    .skill-hexagon {
      background: white;
      border-radius: 15px;
      padding: 2rem;
      margin-bottom: 2rem;
      box-shadow: 0 5px 15px rgba(0,0,0,0.1);
      transition: all 0.4s ease;
      position: relative;
      overflow: hidden;
      border: 2px solid transparent;
    }
    
    .skill-hexagon::before {
      content: '';
      position: absolute;
      top: 50%;
      left: 50%;
      width: 0;
      height: 0;
      background: radial-gradient(circle, rgba(102, 126, 234, 0.08) 0%, transparent 70%);
      border-radius: 50%;
      transform: translate(-50%, -50%);
      transition: width 0.6s ease, height 0.6s ease;
    }
    
    .skill-hexagon:hover::before {
      width: 400px;
      height: 400px;
    }
    
    .skill-hexagon:hover {
      transform: translateY(-8px) scale(1.02);
      box-shadow: 0 20px 40px rgba(102, 126, 234, 0.2);
      border-color: rgba(102, 126, 234, 0.3);
    }
    
    .skill-bar-container {
      height: 16px;
      background: linear-gradient(90deg, #e0e0e0, #f0f0f0);
      border-radius: 10px;
      overflow: hidden;
      position: relative;
    }
    
    .skill-bar {
      height: 100%;
      background: linear-gradient(90deg, #667eea, #764ba2);
      border-radius: 10px;
      transition: width 1.5s cubic-bezier(0.68, -0.55, 0.265, 1.55);
      position: relative;
      overflow: hidden;
    }
    
    .skill-bar::after {
      content: '';
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: linear-gradient(90deg, transparent, rgba(255,255,255,0.3), transparent);
      animation: shimmer 2s infinite;
    }
    
    @keyframes shimmer {
      0% { transform: translateX(-100%); }
      100% { transform: translateX(100%); }
    }
    
    /* Timeline Education */
    .education-section {
      background: white;
      padding: 5rem 0;
      position: relative;
    }
    
    .timeline {
      position: relative;
      padding-left: 4rem;
    }
    
    .timeline::before {
      content: '';
      position: absolute;
      left: 1.5rem;
      top: 0;
      bottom: 0;
      width: 4px;
      background: linear-gradient(180deg, #667eea, #764ba2);
    }
    
    .timeline-item {
      position: relative;
      margin-bottom: 3rem;
    }
    
    .timeline-dot {
      position: absolute;
      left: -2.65rem;
      top: 0;
      width: 30px;
      height: 30px;
      background: linear-gradient(135deg, #667eea, #764ba2);
      border: 4px solid white;
      border-radius: 50%;
      box-shadow: 0 0 0 4px rgba(102, 126, 234, 0.2);
      z-index: 1;
    }
    
    .timeline-badge {
      display: inline-block;
      padding: 0.4rem 1rem;
      background: linear-gradient(135deg, #667eea, #764ba2);
      color: white;
      border-radius: 20px;
      font-size: 0.8rem;
      font-weight: bold;
      margin-bottom: 1rem;
      box-shadow: 0 4px 10px rgba(102, 126, 234, 0.3);
    }
    
    .timeline-card {
      background: white;
      border-radius: 20px;
      padding: 2rem;
      box-shadow: 0 10px 30px rgba(0,0,0,0.1);
      transition: all 0.3s ease;
      border: 2px solid #f0f0f0;
    }
    
    .timeline-card:hover {
      transform: translateX(10px);
      box-shadow: 0 15px 40px rgba(102, 126, 234, 0.2);
      border-color: #667eea;
    }
    
    /* Language Cards with Icons */
    .languages-section {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      padding: 5rem 0;
      color: white;
    }
    
    .language-card {
      background: rgba(255, 255, 255, 0.1);
      backdrop-filter: blur(10px);
      border: 2px solid rgba(255, 255, 255, 0.2);
      border-radius: 20px;
      padding: 2.5rem;
      text-align: center;
      transition: all 0.3s ease;
      height: 100%;
    }
    
    .language-card:hover {
      background: rgba(255, 255, 255, 0.2);
      transform: translateY(-10px) scale(1.05);
      box-shadow: 0 20px 40px rgba(0,0,0,0.3);
    }
    
    .language-icon {
      font-size: 4rem;
      margin-bottom: 1rem;
      display: inline-block;
      animation: bounce 2s infinite;
    }
    
    @keyframes bounce {
      0%, 100% { transform: translateY(0); }
      50% { transform: translateY(-15px); }
    }
    
    /* Contact Section - Horizontal Split Layout */
    .contact-section {
      background: white;
      padding: 5rem 0;
    }
    
    .contact-main-card {
      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
      border-radius: 30px;
      overflow: hidden;
      box-shadow: 0 20px 60px rgba(102, 126, 234, 0.3);
      position: relative;
    }
    
    .contact-left {
      padding: 4rem;
      color: white;
      position: relative;
      z-index: 1;
    }
    
    .contact-left::before {
